<div class="mt-3 flow-root">
    <a href="{% if not paged.first %}{{ pagination.href.previous | url }}{% else %}javascript:void(0){% endif %}" class="float-left bg-white dark:bg-gray-700 font-semibold py-2 px-4 rounded shadow-md text-gray-800 dark:text-gray-400 {% if not paged.first %}cursor-pointer hover:bg-gray-100{% else %}cursor-default invisible text-opacity-75{% endif %}">Previous</a>
    <a href="{% if not paged.last %}{{ pagination.href.next | url }}{% else %}javascript:void(0){% endif %}" class="float-right bg-white dark:bg-gray-700 font-semibold py-2 px-4 rounded shadow-md text-gray-800 dark:text-gray-400 {% if not paged.last %}cursor-pointer hover:bg-gray-100{% else %}cursor-default invisible text-opacity-75{% endif %}">Next</a>
</div>